   "source": [
    "def numJewelsInStones(J, S):\n",
    "    cn=0\n",
    "    for i in S:\n",
    "        for j in J:\n",
    "            if j == i:\n",
    "                cn+=1\n",
    "    return cn\n",
    "print(numJewelsInStones(\"aA\", \"aAAbbbb\"))\n",
    "print(numJewelsInStones(\"z\", \"ZZ\"))\n",
    "# линейное программирование"
